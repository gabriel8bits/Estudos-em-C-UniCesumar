#include <stdio.h>
#include <stdlib.h>

int main()
{
    int num, qtdade, i;

    printf("Informe um numero: ");
    scanf("%d", &num);

    qtdade = 0;

    for (i = 1; i <= num; i++){

        if(num % i == 0)
            qtdade++;
    }

    if (qtdade == 2)
        printf("\nO numero é primo.");
    else
        printf("\nO numero não é primo.");

    return 0;
}
