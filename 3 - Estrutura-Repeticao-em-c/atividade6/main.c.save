#include <stdio.h>
#include <stdlib.h>

int main()
{
    char frase[50];
    int cont, i;

    printf("Digite uma frase qualquer: ");
    gets(frase);

    printf("\nInforme quantas vezes deseja repetir: ");
    scanf("%d", &cont);

    for(i=1; i <= cont; i++) {
        printf("%s\n", frase);
    }

    return 0;
}
